    "material-design-icons": "^3.0.1",
    "ngx-infinite-scroll": "10.0.1",
    "ngx-webstorage": "9.0.0",
    "ngx-xml2json": "^1.0.2",
    "primeicons": "^5.0.0",
    "primeng": "file:../primeng/dist/primeng-28.7.0.tgz",
    "react": "18.2.0",
    "react-dom": "18.2.0",
