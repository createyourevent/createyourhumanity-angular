  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular-builders/jest": "^14.1.0",
    "@angular-devkit/build-angular": "^13.3.8",
    "@angular-eslint/eslint-plugin": "^14.4.0",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.26",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@wisemapping/core-js": "file:../createyourhumanity-mindmap/packages/core-js",
    "@wisemapping/editor": "file:../createyourhumanity-mindmap/packages/editor",
    "@wisemapping/mindplot": "file:../createyourhumanity-mindmap/packages/mindplot",
    "@wisemapping/web2d": "file:../createyourhumanity-mindmap/packages/web2d",
    "babel-loader": "^8.0.6",
    "bootstrap-loader": "^4.0.4",
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "2.3.0",
    "concurrently": "7.0.0",
